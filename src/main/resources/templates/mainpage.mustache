{{>layouts/afterheader}}

<section style="float:left; width:100%; padding:5px;">

{{#list}}
    <div class="parent" style="width: 100%; margin-top: 5%;">
        <div style="width:30%; height: 150px; padding: 10px; padding-left: 150px; float:left; justify-content: center; box-sizing: border-box;">
            <table class="table8_1">
                <form action="/coincharge" method="post">
                    <button type="submit" style="height: 100px">
                        <input type="hidden" value="{{numroom}}" name="numroom">{{numroom}}<br/>
                        <input type="hidden" value="{{iuse}}" name="iuse">{{iuse}}<br/>
                        <input type="hidden" value="{{coin}}" name="coin">코인 : {{coin}}<br/>
                        <input type="hidden" value="{{etime}}" name="etime">시간 : {{etime}}<br/>
                    </button>
                </form>
            </table>
        </div>
    </div>
{{/list}}

</section>
</body>
</html>